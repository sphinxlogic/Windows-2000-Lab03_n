'----------------------------------------------------
'  GetSpecialErrorString returns the error associated
'  with the given error number. These errors are specific
'  to this program and some of the included classes.
'-----------------------------------------------------
Public Function GetSpecialErrorString(ByVal iNum As Integer) As String
 
 Dim sErrDesc As String
 
 Select Case iNum
 
    Case NO_ERROR
       'No need for error description here
       sErrDesc = ""
    Case ERR_UNABLE_TO_LOGON
       sErrDesc = "Unable to Logon."
    Case ERR_INSUFFICENT_ES_PERMISSIONS
       sErrDesc = "User has insufficent event permissions for specified action."
    Case ERR_EMPTY_ROW_IN_MAP
       sErrDesc = "Map has empty row."
    Case ERR_MAP_ROW_INVALID
       sErrDesc = "Map has invalid row or row argument."
    Case ERR_UNABLE_TO_LOGON
       sErrDesc = "Unable to Logon. Try again later."
    Case ERR_INCORRECT_PROFILE_OR_PASSWORD
       sErrDesc = "Password or Profile is incorrect."
    Case ERR_ESCONF_SETUP
       sErrDesc = "Unable to create AgentMap object. Verify that ESCONF.DLL is installed and registered properly."
    Case ERR_FAIL
       sErrDesc = "Unknown error occured while trying to initialize AgentMap object."
    Case ERR_ACTIVITYID_NOT_NUMERIC
       sErrDesc = "Map row acivity ID is not numeric."
    Case ERR_MAP_FLAG_NOT_NUMERIC
       sErrDesc = "Map flag is not numeric."
    Case ERR_MAP_FLAG_OUT_OF_RANGE
       sErrDesc = "Map flag is out of range (must be 0,1 or 2)."
    Case ERR_MAP_ACTION_NOT_STRING
       sErrDesc = "Action parm contains numeric characters."
    Case ERR_INSERT_ROW_NO_VALID_MAP
       sErrDesc = "Tried to insert a row but there wasn't a valid map"
    Case ERR_EXRTOBJ_SETUP
       sErrDesc = "EXRTOBJ.DLL doesn't exist or isn't registered properly."
    Case ERR_MAP_MSG_DOESNT_EXIST
       sErrDesc = "Map msg doesn't exist, can't get map."
    Case ERR_MAP_AGENT_DOESNT_EXIST
       sErrDesc = "Agent does not exist."
    Case ERR_ARGUMENT_ERROR
       sErrDesc = "An argument passed to GetMap is wrong."
    Case ERR_UNABLE_TO_ADD_ROUTING_MAP_MSG
       sErrDesc = "Unable to add routing map message to message collection."
    Case ERR_ROUTING_AGENT_ALREADY_EXISTS
       sErrDesc = "Can't create because routing agent already exists in specified folder."
    Case ERR_CANT_LOGON
       sErrDesc = "Unable to Logon using Servername and Mailbox information given."
    Case ERR_PERMISSIONS_DENIED
       sErrDesc = "Insufficent Event Permissions to Modify Agents."
    Case Else  'Note: This is a code failure that a string hasn't been assign to a number, should not occur.
       sErrDesc = "Unmatched Error Description. An error occured that doesn't have a corresponding error message."
   End Select
   
    GetSpecialErrorString = sErrDesc
    
End Function
