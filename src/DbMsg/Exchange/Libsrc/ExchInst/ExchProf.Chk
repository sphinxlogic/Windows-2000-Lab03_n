//$--CHK_HrMAPIProfileExists-----------------------------------------------------
//  Check the parameters for HrMAPIProfileExists().
// -----------------------------------------------------------------------------
__inline static HRESULT CHK_HrMAPIProfileExists(
    IN LPPROFADMIN lpProfAdmin,         // profile admin object
    IN LPSTR      lpszProfileName)     // profile name
