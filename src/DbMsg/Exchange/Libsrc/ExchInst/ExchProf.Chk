//$--CHK_HrSetGatewayProfileParameters------------------------------------------
//  Check the parameters for HrSetGatewayProfileParameters().
// -----------------------------------------------------------------------------
__inline static HRESULT CHK_HrSetGatewayProfileParameters(
    IN LPSTR lpszServerName,                   // server name
    IN LPSTR lpszSiteDN,                       // site distinguished name
    IN LPSTR lpszCommonName)                   // common name
{
    HRESULT hr = NOERROR;

    if(! TEST_STRINGA_PTR(lpszServerName))
    {
        hr = HR_LOG(E_INVALIDARG);
        goto cleanup;
    }

    if(! TEST_STRINGA_PTR(lpszSiteDN))
    {
        hr = HR_LOG(E_INVALIDARG);
        goto cleanup;
    }

    if(! TEST_STRINGA_PTR(lpszCommonName))
    {
        hr = HR_LOG(E_INVALIDARG);
        goto cleanup;
    }

cleanup:

    return(hr);
}

